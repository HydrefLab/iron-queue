{
    "name": "hydreflab/iron-queue",
    "description": "IronMQ support for the Laravel queue.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hydreflab/iron-queue/issues",
        "source": "https://github.com/hydreflab/iron-queue"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/container": "5.6.*",
        "illuminate/contracts": "5.6.*",
        "illuminate/encryption": "5.6.*",
        "illuminate/http": "5.6.*",
        "illuminate/queue": "5.6.*",
        "illuminate/support": "5.6.*",
        "iron-io/iron_mq": "~4.0",
        "jeremeamia/superclosure": "~2.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Collective\\IronQueue\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Collective\\IronQueue\\IronQueueServiceProvider"
            ]
        }
    }
}
